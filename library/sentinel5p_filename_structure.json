{
  "mission": {
    "pattern": "^S5P$",
    "description": "Sentinel-5 Precursor mission (TROPOMI instrument)"
  },
  "instrument": {
    "pattern": "^TROPOMI$",
    "description": "Tropospheric Monitoring Instrument"
  },
  "processing_level": {
    "pattern": "^(L0|L1A|L1B|L1C|L2A)$",
    "description": "Processing level code",
    "details": {
      "L1B": "Level-1B calibrated radiances",
      "L2": "Level-2 geophysical products"
    }
  },
  "product_type": {
    "pattern": "^[A-Z0-9_]{3,20}$",
    "description": "Product type identifier, varies by observed species (e.g., NO2, O3, CH4) and processing"
  },
  "start_datetime": {
    "pattern": "^(?:19|20)\\d\\d(?:0[1-9]|1[0-2])(?:0[1-9]|[12]\\d|3[01])T(?:[01]\\d|2[0-3])(?:[0-5]\\d){2}$",
    "description": "UTC date and time when acquisition started, format YYYYMMDDThhmmss, valid ranges enforced"
  },
  "stop_datetime": {
    "pattern": "^(?:19|20)\\d\\d(?:0[1-9]|1[0-2])(?:0[1-9]|[12]\\d|3[01])T(?:[01]\\d|2[0-3])(?:[0-5]\\d){2}$",
    "description": "UTC date and time when acquisition ended, format YYYYMMDDThhmmss, valid ranges enforced"
  },
  "absolute_orbit": {
    "pattern": "^\\d{6}$",
    "description": "Absolute orbit number (6 digits)"
  },
  "product_unique_id": {
    "pattern": "^[0-9A-F]{4,12}$",
    "description": "Unique product identifier (hexadecimal, length 4\u201312)"
  },
  "extension": {
    "pattern": "^\\.S5P$",
    "description": "Sentinel-5P product package format"
  },
  "valid_example": "S5P_TROPOMI_L2_NO2_20250105T103021_20250105T105021_045678_ABCD1234.S5P",
  "filename_pattern": "^(?P<mission>S5P)_(?P<instrument>TROPOMI)_(?P<processing_level>L1B|L2)_(?P<product_type>[A-Z0-9_]{3,20})_(?P<start_datetime>(?:19|20)\\d\\d(?:0[1-9]|1[0-2])(?:0[1-9]|[12]\\d|3[01])T(?:[01]\\d|2[0-3])(?:[0-5]\\d){2})_(?P<stop_datetime>(?:19|20)\\d\\d(?:0[1-9]|1[0-2])(?:0[1-9]|[12]\\d|3[01])T(?:[01]\\d|2[0-3])(?:[0-5]\\d){2})_(?P<absolute_orbit>\\d{6})_(?P<product_unique_id>[0-9A-F]{4,12})(?P<extension>\\.S5P)$",
  "fields_order": [
    "mission",
    "instrument",
    "processing_level",
    "product_type",
    "start_datetime",
    "stop_datetime",
    "absolute_orbit",
    "product_unique_id",
    "extension"
  ]
}